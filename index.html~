    <script>
    $(document).ready(function() {
        $("input").keydown(function(event) {
            if (event.keyCode === 13) {
                event.preventDefault();
                return false;
            }
        });
        $(".postal-code").keydown(function(event) {
            if (event.keyCode === 38 || event.keyCode === 40) {
                event.preventDefault();
                return false;
            }
        });
        $(".mob-input").keydown(function(event) {
            if (event.keyCode === 38 || event.keyCode === 40) {
                event.preventDefault();
                return false;
            }
        });

    });
    </script>
